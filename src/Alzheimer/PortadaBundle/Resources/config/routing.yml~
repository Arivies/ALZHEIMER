usuario_index:
    pattern:  /admin/usuario
    defaults: { _controller: UsuarioBundle:Usuario:index }

_usuario_index:
    pattern:  /admin/usuario/
    defaults: { _controller: UsuarioBundle:Usuario:index }

usuario_new:
    pattern:  /admin/usuario/new
    defaults: { _controller: UsuarioBundle:Usuario:new }

_usuario_new:
    pattern:  /admin/usuario/new/
    defaults: { _controller: UsuarioBundle:Usuario:new }

usuario_delete:
    pattern:  /admin/usuario/delete/{id}
    defaults: { _controller: UsuarioBundle:Usuario:delete }

_usuario_delete:
    pattern:  /admin/usuario/delete/{id}/
    defaults: { _controller: UsuarioBundle:Usuario:delete }

usuario_edit:
    pattern:  /admin/usuario/edit/{id}
    defaults: { _controller: UsuarioBundle:Usuario:edit }

_usuario_edit:
    pattern:  /admin/usuario/edit/{id}/
    defaults: { _controller: UsuarioBundle:Usuario:edit }

usuario_show:
    pattern:  /admin/usuario/show/{id}
    defaults: { _controller: UsuarioBundle:Usuario:show }

_usuario_edit:
    pattern:  /admin/usuario/show/{id}/
    defaults: { _controller: UsuarioBundle:Usuario:show }

usuario_update:
    pattern:  /admin/usuario/update/{id}
    defaults: { _controller: UsuarioBundle:Usuario:update }

